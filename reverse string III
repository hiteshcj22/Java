class Solution {
    public String reverseWords(String s) {
       String[] strArray = null;  
        strArray = s.split(" "); 
        String c="";
        for(int i=0;i<strArray.length;i++){
        for(int j=strArray[i].length()-1;j>=0;j--){
            c=c+strArray[i].charAt(j) ;
        }
                     
        c=c+" "; 
        }
        return c.trim();
        
    }
}
